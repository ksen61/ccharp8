<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">

    <v:String x:Key="SerializeButton">Сериализовать</v:String>
    <v:String x:Key="DeserializeButton">Десериализовать</v:String>
    <v:String x:Key="SerialMessage">Данные успешно сериализованы!</v:String>
    <v:String x:Key="SerialFail">Ошибка сериализации: {0}</v:String>
    <v:String x:Key="DeserialMessage">Данные успешно десериализованы!</v:String>
    <v:String x:Key="DeserialFail">Ошибка десериализации: {0}</v:String>
    <v:String x:Key="SerialNoFile">Файл сериализации не найден.</v:String>
</ResourceDictionary>
