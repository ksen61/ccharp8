<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">
    
    <v:String x:Key="SerializeButton">Serialize</v:String>
    <v:String x:Key="DeserializeButton">Deserialize</v:String>
    <v:String x:Key="SerialMessage">Data serialized successfully!</v:String>
    <v:String x:Key="SerialFail">Serialization failed: {0}</v:String>
    <v:String x:Key="DeserialMessage">Data deserialized successfully!</v:String>
    <v:String x:Key="DeserialFail">Deserialization failed: {0}</v:String>
    <v:String x:Key="SerialNoFile">Serialized file not found.</v:String>
</ResourceDictionary>
